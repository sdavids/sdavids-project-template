{
  "name": "sdavids-project-template",
  "description": "",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "contributors": [
    "Sebastian Davids <sdavids@gmx.de>"
  ],
  "homepage": "https://github.com/sdavids/sdavids-project-template",
  "bugs": "https://github.com/sdavids/sdavids-project-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sdavids/sdavids-project-template.git"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "scripts/macos_node_modules_fix.sh",
    "prepare": "scripts/prepare.sh",
    "check:types": "tsc --project tsconfig.prod.json && tsc --project vitest/tsconfig.prod.json",
    "check:types:node": "tsc --project tsconfig.prod.json",
    "check:types:src": "tsc --project src/tsconfig.prod.json",
    "check:types:vitest": "tsc --project vitest/tsconfig.prod.json",
    "clean": "scripts/clean.sh",
    "clean:node": "scripts/clean_node.sh",
    "dependency:updates": "scripts/dependency_check_node.sh",
    "dev": "vite",
    "build": "tsc --project src/tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "format": "scripts/format.sh",
    "format:check": "scripts/format_check.sh",
    "lint": "scripts/lint.sh",
    "lint:fix": "scripts/lint_fix.sh",
    "test": "vitest run",
    "test:ci": "vitest run --reporter default --reporter json --outputFile reports/vitest/test-output.json",
    "test:verbose": "vitest run --reporter verbose",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@eslint/css": "0.8.1",
    "@eslint/js": "9.27.0",
    "@eslint/json": "0.12.0",
    "@prettier/plugin-xml": "3.4.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@types/node": "22.15.19",
    "@vitest/eslint-plugin": "1.2.0",
    "browserslist": "4.24.5",
    "browserslist-config-baseline": "0.4.0",
    "browserslist-to-esbuild": "2.1.1",
    "eslint": "9.27.0",
    "eslint-import-resolver-typescript": "4.3.5",
    "eslint-plugin-depend": "1.2.0",
    "eslint-plugin-import-x": "4.12.2",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-testing-library": "7.2.1",
    "globals": "16.1.0",
    "happy-dom": "17.4.7",
    "husky": "9.1.7",
    "jiti": "2.4.2",
    "lightningcss": "1.30.1",
    "lint-staged": "16.0.0",
    "prettier": "3.5.3",
    "terser": "5.39.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1",
    "vite": "6.3.5",
    "vite-plugin-csp-guard": "2.1.1",
    "vite-plugin-minify": "2.1.0",
    "vite-plugin-sitemap": "0.8.2",
    "vitest": "3.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "4.41.0",
    "@rollup/rollup-darwin-x64": "4.41.0",
    "@rollup/rollup-linux-arm64-gnu": "4.41.0",
    "@rollup/rollup-linux-x64-gnu": "4.41.0"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">= 22.15"
    },
    "packageManager": {
      "name": "npm",
      "version": ">= 10.9"
    }
  },
  "browserslist": [
    "extends browserslist-config-baseline"
  ],
  "browserslist-config-baseline": {
    "baselineYear": 2022
  }
}
